{
  "compilerOptions": {
      "target": "ES6",
      "esModuleInterop": true,
      "outDir": "./dist",
      "module": "commonjs",
      "moduleResolution": "Node",
      "baseUrl": "src",
      "strict": true,
      "skipLibCheck": true,           
      "forceConsistentCasingInFileNames": true,
      "rootDir": "./src"
  },
  "include": ["src/**/*"]           
}
